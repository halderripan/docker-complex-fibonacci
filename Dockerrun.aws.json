{
    "AWSEBDockerrunVersion": 2,
    "containerDefinitions": [
        {
            "name": "client",
            "image": "halderripan/complex-fibonacci-client",
            "hostname": "client",
            "essential": false,
            "memory": 128
        },
        {
            "name": "worker",
            "image": "halderripan/complex-fibonacci-worker",
            "hostname": "worker",
            "essential": false,
            "memory": 128
        },
        {
            "name": "server",
            "image": "halderripan/complex-fibonacci-server",
            "hostname": "api",
            "essential": false,
            "memory": 128
        },
        {
            "name": "nginx",
            "image": "halderripan/complex-fibonacci-nginx",
            "hostname": "nginx",
            "essential": true,,
            "memory": 128,
            "portMappings":[
                {
                    "hostPort": 80,
                    "containerPort": 80
                }
            ],
            "links": ["server", "client"]
        }
    ]
}